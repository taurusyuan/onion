<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="io.renren.modules.onion.dao.FeedbackTypeDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="io.renren.modules.onion.entity.FeedbackTypeEntity" id="feedbackTypeMap">
        <result property="id" column="id"/>
        <result property="typeName" column="type_name"/>
    </resultMap>

	<select id="queryObject" resultType="io.renren.modules.onion.entity.FeedbackTypeEntity">
		select * from feedback_type where id = #{value}
	</select>

	<select id="queryList" resultType="io.renren.modules.onion.entity.FeedbackTypeEntity">
		select * from feedback_type
        <choose>
            <when test="sidx != null and sidx.trim() != ''">
                order by ${sidx} ${order}
            </when>
			<otherwise>
                order by id desc
			</otherwise>
        </choose>
		<if test="offset != null and limit != null">
			limit #{offset}, #{limit}
		</if>
	</select>
	
 	<select id="queryTotal" resultType="int">
		select count(*) from feedback_type 
	</select>
	 
	<insert id="save" parameterType="io.renren.modules.onion.entity.FeedbackTypeEntity" useGeneratedKeys="true" keyProperty="id">
		insert into feedback_type
		(
-- 			`type_name`
			type_name
		)
		values
		(
			#{typeName}
		)
	</insert>
	 
	<update id="update" parameterType="io.renren.modules.onion.entity.FeedbackTypeEntity">
		update feedback_type 
		<set>
			<if test="typeName != null">`type_name` = #{typeName}</if>
		</set>
		where id = #{id}
	</update>
	
	<delete id="delete">
		delete from feedback_type where id = #{value}
	</delete>
	
	<delete id="deleteBatch">
		delete from feedback_type where id in 
		<foreach item="id" collection="array" open="(" separator="," close=")">
			#{id}
		</foreach>
	</delete>

</mapper>